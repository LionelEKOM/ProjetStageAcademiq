{% extends 'base.html.twig' %}

{% block title %}Examen index{% endblock %}

{% block body %}
    <h1>Examen index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>CreatedAt</th>
                <th>Type</th>
                <th>Resultat</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for examan in examens %}
            <tr>
                <td>{{ examan.id }}</td>
                <td>{{ examan.createdAt ? examan.createdAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ examan.type }}</td>
                <td>{{ examan.resultat }}</td>
                <td>
                    <a href="{{ path('app_examen_show', {'id': examan.id}) }}">show</a>
                    <a href="{{ path('app_examen_edit', {'id': examan.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_examen_new') }}">Create new</a>
{% endblock %}
